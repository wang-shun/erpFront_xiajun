{
  "entry": "src/index.js",
  "disableCSSModules": false,
  "less": true,
  "publicPath": "/",
  "autoprefixer": null,
  "proxy": {
    "/": {
      "target": "http://test.buyer007.cn",
      "changeOrigin": true,
      "pathRewrite": { "^/" : "" }
    }
  },
  "extraBabelPlugins": [
    "transform-runtime",
    "transform-decorators-legacy",
    ["import", { "libraryName": "antd", "style": true }]
  ],
  "theme": {
    "primary-color": "#00cbd7"
  },
  "externals": {
    "react": "window.React",
    "react-dom": "window.ReactDOM"
  },
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr"
      ]
    },
    "production": {
      "extraBabelPlugins": [
        "transform-remove-console-enhance"
      ]
    }
  }
}
